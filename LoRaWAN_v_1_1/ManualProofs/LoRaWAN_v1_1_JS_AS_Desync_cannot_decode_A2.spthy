            SessionKey(senc(<'0x01', Nonce(<$DevEUI, ('2'+ctr_JS_in)>), 
                             $JoinEUI, DevNonce, 'pad_with_0s'>,
                            AesKey('Nwk', ~rootNwkKey))), 
            SessionKey(senc(<'0x03', Nonce(<$DevEUI, ('2'+ctr_JS_in)>), 
                             $JoinEUI, DevNonce, 'pad_with_0s'>,
                            AesKey('Nwk', ~rootNwkKey))), 
            SessionKey(senc(<'0x04', Nonce(<$DevEUI, ('2'+ctr_JS_in)>), 
                             $JoinEUI, DevNonce, 'pad_with_0s'>,
                            AesKey('Nwk', ~rootNwkKey)))
