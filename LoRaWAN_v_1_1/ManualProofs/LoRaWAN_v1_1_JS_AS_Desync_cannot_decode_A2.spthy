          <
           <'Join_Accept', $DevEUI, 
            senc(<Nonce(<$DevEUI, ('2'+ctr_JS_in)>), 'NetID', 
                  Nonce(~rnd32DevAddr), 'opt_params', 
                  MAC(<'0xFF', $JoinEUI, DevNonce, 'MHDR', 
                       Nonce(<$DevEUI, ('2'+ctr_JS_in)>), 'NetID', Nonce(~rnd32DevAddr), 
